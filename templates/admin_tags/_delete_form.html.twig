<form method="post" action="{{ path('app_admin_tags_delete', {'id': tag.id}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ tag.id) }}">
    <button class="btn btn-danger btn-round">Delete</button>
</form>
