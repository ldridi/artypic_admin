{% extends 'base.html.twig' %}

{% block title %}Dimensions{% endblock %}

{% block body %}
    <h1>Dimensions</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ dimension.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ dimension.name }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ dimension.status ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_dimensions_index') }}">back to list</a>

    <a href="{{ path('app_admin_dimensions_edit', {'id': dimension.id}) }}">edit</a>

    {{ include('admin_dimensions/_delete_form.html.twig') }}
{% endblock %}
