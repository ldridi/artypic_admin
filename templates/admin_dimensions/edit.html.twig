{% extends 'baseAdmin.html.twig' %}

{% block title %}Modification Dimensions{% endblock %}

{% block body %}


    <div class="content">
        <div class="row">

            <div class="col-md-8">
                <div class="card card-user">
                    <div class="card-header">
                        <h5 class="card-title" style="color:#EF857D; font-weight: bold">Modification de la dimension "{{ dimension.name}}" <a href="{{ path('app_admin_dimensions_index') }}">
                                <button type="button" rel="tooltip" class="btn btn-info btn-icon btn-md " data-original-title="" title="">
                                    <i class="fa fa-home"></i>
                                </button></a></h5>
                    </div>
                    <div class="card-body">
                        {{ include('admin_dimensions/_form.html.twig', {'button_label': 'Sauvegarder'}) }}
                        {{ include('admin_dimensions/_delete_form.html.twig') }}
                    </div>
                </div>
            </div>
        </div>
    </div>

{% endblock %}
